{
  "timestamp": {
    "description": "Unix-timestamp: количество секунд с начала 1970 года",
    "type": "integer",
    "minimum": 0,
    "maximum": 9999999999
  },
  "tag": {
    "description": "Тэг. Короткая строка из 2-20 символов",
    "type": "string",
    "minLength": 2,
    "maxLength": 20
  },
  "photo-url": {
    "description": "Ссылка на изображение. Не более 500 символов",
    "type": "string",
    "maxLength": 500
  },
  "video": {
    "description": "Видео. Объект, содержащий поля url, preview_url и video_type_id",
    "type": "object",
    "required": [
      "url",
      "video_type_id"
    ],
    "properties": {
      "url": {
        "type": "string",
        "maxLength": 500
      },
      "preview_url": {
        "type": "string",
        "maxLength": 500
      },
      "video_type_id": {
        "type": "integer"
      }
    }
  }
}